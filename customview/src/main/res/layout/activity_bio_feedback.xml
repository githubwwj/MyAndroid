<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#03A9F4">

    <cn.wwj.customview.widget.BioFeedbackView
        android:id="@+id/waterRipple"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginHorizontal="10dp"
        android:background="#ffffff"
        android:scaleType="centerInside"
        app:bioBorderLineColor="#FF5722"
        app:bioBorderWidth="1dp"
        app:bioBottomFirstLineColor="@color/bioBottomFirstLineColor"
        app:bioBottomSecondLineColor="@color/bioBottomSecondLineColor"
        app:bioCommitTextColor="@color/bioCommitTextColor"
        app:bioIsDrawVerticalLine="true"
        app:bioTitle="尿急-功能性训练"
        app:bioTextColor="@color/bioTextColor"
        app:bioTitleColor="@color/bioTitleColor" />

</RelativeLayout>